{
	"name": "cloudformation-ext-resourcespec-vscode",
	"displayName": "cloudformation-ext-resourcespec-vscode",
	"repository": {
		"url": "https://github.com/bg46z/cloudformation-ext-resourcespec-vscode"
	},
	"publisher": "zbridges",
	"description": "CloudFormation Snippets based on AWS resource specification",
	"version": "0.4.0",
	"engines": {
		"vscode": "^1.36.0"
	},
	"categories": [
		"Other",
		"Snippets"
	],
	"activationEvents": [
		"onLanguage:yaml",
		"workspaceContains:**/*.yml",
		"workspaceContains:**/*.yaml"
	],
	"main": "./out/extension.js",
	"contributes": {},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.36.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"vscode-test": "^1.0.2"
	},
	"dependencies": {
		"stream-json": "^1.3.0"
	}
}
